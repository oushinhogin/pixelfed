#!/bin/bash

set -e
echo "Running composer install (${COMMIT_ID})"

ls -l storage
ls -l ../..

composer \
    --no-ansi \
    --no-interaction \
    --optimize-autoloader \
    --no-progress \
    --no-dev \
    --profile \
    install

php artisan migrate --no-interaction --no-ansi --force

php artisan storage:link

npm install
