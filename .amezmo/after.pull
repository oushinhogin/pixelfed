#!/bin/bash
set -e;
echo "Running composer install (${COMMIT_ID})";
npm install;

composer \
    --no-ansi \
    --no-interaction \
    --optimize-autoloader \
    --no-progress \
    --no-dev \
    --profile \
    install

php artisan migrate --no-interaction --no-ansi --force;
php artisan user:admin josh
ls -l
